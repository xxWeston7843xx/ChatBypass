local function LZKC_fake_script() -- byjhegjfhgjw.hkjsehgkjhw 
	local script = Instance.new('LocalScript', byjhegjfhgjw)

	local main = script.Parent.Parent.Parent.mainfrakjwhgkjh
	local credits = script.Parent.Parent.Parent.credkjhwgkjh
	
	script.Parent.Parent.byjhegjfhgjw.MouseButton1Click:Connect(function()
		main.Visible = true
		credits.Visible = false
	end)
end
